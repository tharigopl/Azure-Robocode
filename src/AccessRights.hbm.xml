<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.utd.robocode.dto">

 <class name="AccessRights" table="access_rights">
    <id name="ar_id" column="ar_id">
      <generator class="identity" />
    </id>
    <property name="ar_robot_id" column="ar_robot_id" />
    <property name="ar_domain_id" column="ar_domain_id" />
    <property name="ar_role_id" column="ar_role_id" />
    <property name="ar_right_id" column="ar_right_id" />
    <property name="created_date" column="created_date" />
    <property name="updated_date" column="updated_date" />
 </class>

</hibernate-mapping>